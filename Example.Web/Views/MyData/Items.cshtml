

<table class="table table-condensed table-hover table-striped">

    <thead>
        <tr>
            <td><strong>Id</strong></td>
            <td><strong>Name</strong></td>
            <td><strong>LastName</strong></td>
        </tr>
    </thead>

    <tr ng-repeat="item in MyDataItemsController.Items" style="cursor:pointer" ui-sref="mydata.edit({ Id: item.Id })">
        <td>{{ item.Id }}</td>
        <td>{{ item.Name }}</td>
        <td>{{ item.LastName }}</td>
    </tr>

</table>